<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="postgresql-PU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.pruebas.entidades.Registro</class>
    <class>com.pruebas.entidades.PermisoEmpleado</class>
    <class>com.pruebas.entidades.Vista</class>
    <class>com.pruebas.entidades.Horario</class>
    <class>com.pruebas.entidades.Permiso</class>
    <class>com.pruebas.entidades.Empleado</class>
    <class>com.pruebas.entidades.CambioTurno</class>
    <class>com.pruebas.entidades.Jornada</class>
    <class>com.pruebas.entidades.TCImportacion</class>
    <class>com.pruebas.entidades.Tardanza</class>
    <class>com.pruebas.entidades.Falta</class>
    <class>com.pruebas.entidades.EmpleadoHorario</class>
    <class>com.pruebas.entidades.HorarioJornada</class>
    <class>com.pruebas.entidades.Feriado</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://127.0.0.1:5432/pruebabd"/>
      <property name="javax.persistence.jdbc.user" value="postgres"/>
      <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
      <property name="javax.persistence.jdbc.password" value="root"/>
    </properties>
  </persistence-unit>
</persistence>
